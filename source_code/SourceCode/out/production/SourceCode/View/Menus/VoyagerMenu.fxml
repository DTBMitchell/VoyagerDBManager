<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MenuAnchor" prefHeight="429.0" prefWidth="148.0" stylesheets="@../logIn.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MenuController">
   <children>
      <VBox fx:id="tabsVBox" layoutX="10.0" layoutY="81.0" prefHeight="429.0" prefWidth="225.0" styleClass="pane" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox fx:id="TabsHBox" alignment="CENTER" prefHeight="46.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" prefHeight="46.0" prefWidth="85.0" styleClass="text-class" text="Logged in as:" textFill="WHITE" />
                  <Label fx:id="NameHolder" prefHeight="46.0" prefWidth="108.0" styleClass="text-class" text="NAME_HOLDER" textFill="WHITE">
                     <HBox.margin>
                        <Insets right="35.0" />
                     </HBox.margin></Label>
               </children>
            </HBox>
            <Button fx:id="UserList" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#setUserList" prefHeight="50.0" prefWidth="225.0" text="User List">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="EQList" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#setEQList" prefHeight="50.0" prefWidth="225.0" text="Equipment List">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="AssignList" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#setAssignList" prefHeight="50.0" prefWidth="225.0" text="Assignment List">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Button fx:id="collapseButton" alignment="CENTER" mnemonicParsing="false" onAction="#collapseMenu" prefHeight="33.0" prefWidth="41.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
   </children>
</AnchorPane>
